# Makefile.in for BASIC driver
VPATH=@srcdir@
srcdir=@srcdir@
top_srcdir=@top_srcdir@
top_builddir=@top_builddir@
prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@
libexecdir = @libexecdir@
CC = @CC@
EXEEXT = @EXEEXT@
CFLAGS = @CFLAGS@
CPPFLAGS = @CPPFLAGS@ -DLIBEXECDIR=\"$(libexecdir)\"
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@
INSTALL = @INSTALL@
INSTALL_PROGRAM = @INSTALL_PROGRAM@

DEST=basic$(EXEEXT)

all: $(DEST)

$(DEST): $(srcdir)/basic.c
	$(CC) $(CPPFLAGS) $(CFLAGS) $(LDFLAGS) -o $@ $(srcdir)/basic.c $(LIBS)

install: all
	test -d "$(DESTDIR)$(bindir)" || mkdir -p "$(DESTDIR)$(bindir)"
	$(INSTALL_PROGRAM) $(DEST) "$(DESTDIR)$(bindir)/$(DEST)"

clean:
	rm -f $(DEST) *.o

distclean: clean
	rm -f Makefile

.PHONY: all install clean distclean
